{
  "name": "z-progress-loading",
  "version": "0.1.8",
  "homepage": "https://github.com/shallinta/z-progress-loading",
  "files": [
    "dist"
  ],
  "main": "dist/z-progress-loading.common.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name z-progress-loading ./src/index.vue",
    "prepare": "npm run build",
    "release": "standard-version && npm publish",
    "postrelease": "git push && git push origin --tags",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "4.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-service": "4.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "standard-version": "^7.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "keywords": [
    "vue",
    "vuejs",
    "progress",
    "loading",
    "canvas"
  ],
  "description": "Vue progress loading component",
  "license": "MIT"
}
